        function initMap() {
            var locationsSolicitados = [
                 @Html.Raw(ViewBag.MAPSolicitado)
                /*
                 * Next point on map
                 *   -Notice how the last number within the brackets incrementally increases from the prior marker
                 *   -Use http://itouchmap.com/latlong.html to get Latitude and Longitude of a specific address
                 *   -Follow the model below:
                 *      ['<b>Name 3</b><br>Address Line 1<br>City, ST Zipcode<br>Phone: ###-###-####<br><a href="#" target="_blank">Link<a> of some sort.', ##.####, -##.####, #]
                 */
            ];

            var locationsAguardando = [
                 @Html.Raw(ViewBag.MAPAguardando)
         
            ];

            var locationsPronto = [
                 @Html.Raw(ViewBag.MAPPronto)
         
            ];
            var locationsCancelado= [
                 @Html.Raw(ViewBag.MAPCancelado)
         
            ];

            var map = new google.maps.Map(document.getElementById('map'), {
                zoom: 11,

                /* Zoom level of your map */

                center: new google.maps.LatLng(-12.953602, -38.444921)

                ,

                    /* coordinates for the center of your map */
                    mapTypeId: google.maps.MapTypeId.ROADMAP
                });

            var infowindow = new google.maps.InfoWindow();

            var markerAzul, markerAmarelo, markerVerde, markerVermelho, i;
            var imageAzul = '../Content/AppBarragem.WEB.ADM/img/pin_azul_tratado.png';
            var imageAmarelo = '../Content/AppBarragem.WEB.ADM/img/pin_amarelo_tratado.png';
            var imageVerde = '../Content/AppBarragem.WEB.ADM/img/pin_verde_tratado.png';
            var imageVermelho = '../Content/AppBarragem.WEB.ADM/img/pin_vermelho_tratado.png';

            for (i = 0; i < locationsSolicitados.length; i++) {
                markerAzul  = new google.maps.Marker({
                    zoom: 10,
                    position: new google.maps.LatLng(locationsSolicitados[i][1], locationsSolicitados[i][2]),
                    map: map,
                    icon: imageAzul 
                });

                google.maps.event.addListener(markerAzul, 'click', (function (marker, i) {
                    return function () {
                        infowindow.setContent(locationsSolicitados[i][0]);
                        infowindow.open(map, markerAzul);
                    }
                })(markerAzul, i));
            }


            for (i = 0; i < locationsAguardando.length; i++) {
                markerAmarelo = new google.maps.Marker({
                    zoom: 10,
                    position: new google.maps.LatLng(locationsAguardando[i][1], locationsAguardando[i][2]),
                    map: map,
                    icon: imageAmarelo
                });

                google.maps.event.addListener(markerAmarelo, 'click', (function (markerAmarelo, i) {
                    return function () {
                        infowindow.setContent(locationsAguardando[i][0]);
                        infowindow.open(map, markerAmarelo);
                    }
                })(markerAmarelo, i));
            }



            for (i = 0; i < locationsPronto.length; i++) {
                markerVerde = new google.maps.Marker({
                    zoom: 10,
                    position: new google.maps.LatLng(locationsPronto[i][1], locationsPronto[i][2]),
                    map: map,
                    icon: imageVerde
                });

                google.maps.event.addListener(markerVerde, 'click', (function (markerVerde, i) {
                    return function () {
                        infowindow.setContent(locationsPronto[i][0]);
                        infowindow.open(map, markerVerde);
                    }
                })(markerVerde, i));
            }


            for (i = 0; i < locationsCancelado.length; i++) {
                markerVermelho = new google.maps.Marker({
                    zoom: 10,
                    position: new google.maps.LatLng(locationsCancelado[i][1], locationsCancelado[i][2]),
                    map: map,
                    icon: imageVermelho
                });

                google.maps.event.addListener(markerVermelho, 'click', (function (markerVermelho, i) {
                    return function () {
                        infowindow.setContent(locationsCancelado[i][0]);
                        infowindow.open(map, markerVermelho);
                    }
                })(markerVermelho, i));
            }
        }
        google.maps.event.addDomListener(window, 'load', initMap);


    });



