
@if (@TempData["ATENCAO"] != null)
{
    <div class="alert alert-danger alert-dismissible" style="position:fixed;width: 89.9%;z-index:99;
    margin: -46% 0% 0% 0%;">
        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
        <h4> Alerta!</h4>


        @TempData["ATENCAO"].ToString()


    </div>
}



@if (@TempData["SUCESSO"] != null)
{
    <div class="alert alert-success alert-dismissible" style="position:fixed;width: 89.9%; z-index:99;
    margin: 2% 0% 0% 0%;">
        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
        <h4> Mensagem!</h4>


        @TempData["SUCESSO"].ToString()


    </div>
}

<div class="alert alert-danger alert-dismissible" style="position:fixed;width: 89.9%;z-index:99;
