@model IEnumerable<ZeNerd.DAL.Context.Configuracao>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutCrud.cshtml";
}

<h2>Index</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ValorFrete)
        </th>
     
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ValorFrete)
            </td>
         
            <td>
                @Html.ActionLink(" ", "Edit", new { id = item.Id }, new { @style = "width:25%;", @class = "btn btn-warning fa fa-pencil" }) 
         
            </td>
        </tr>
    }

</table>
